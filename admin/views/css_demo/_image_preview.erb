<div class="card">
    <div class="card-title">
        <h4 class="underline">多图片预览</h4>
    </div>

    <div class="form-group">
        <label>图片上传(多张)</label>
        <div class="image-preview">
            <div class="img-upload">
                <div class="img-upload-mask">
                    <%= file_field_tag :file, :multiple => true, :class => 'inbox-file' %>
                </div>
            </div>
        </div>
    </div>
</div>